public class Sorts{
    public static void bubble(int[]c){
	int x = 0;
	for(int n = c.length(); n > 0; n --){
	    for(int i = 0; i < n; i ++){
		if (c[i] < c[i + 1]){
		    x = c[i];
		    c[i] = c[i+1];
		    c[i+1] = x;
		}
	    }
	} 
    }

    public static main void(String[]args){
	int[] a = new int [10];
	System.out.println(a);
	a = {3,7,1,3,78,34,1,0,9,9};
	bubble(a);
	System.out.println(a);
    }
}